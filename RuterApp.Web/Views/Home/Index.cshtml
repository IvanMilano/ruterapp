@{
    ViewBag.Title = "Home Page";
}

@using RuterApp.Web.Extensions
@model RuterApp.Web.HoldeplassInfoViewModel
@section scripts
{
    @Html.InitPageMainModule("RuterAppMain")
}
<br/><br/>
<table width="100%">
    <thead>
        <tr>
            <th style="width:20%">Navn</th>
            <th style="width:15%">Linje navn</th>
            <th style="width:15%">Retning</th>
            <th style="width:15%">Holdeplass</th>
            <th style="width:15%">Tid til ankomst</th>
            <th style="width:20%">Forsinkelse</th>
        </tr>
    </thead>
    <tbody data-bind="foreach: ruter">
        <tr>
            <td data-bind="text: navn"></td>
            <td data-bind="text: linjeNavn"></td>
            <td data-bind="text: retning"></td>
            <td data-bind="text: holdeplaass"></td>
            <td data-bind="text: tidTilAnkomst"></td>
            <td data-bind="text: forsinkelse"></td>
        </tr>
    </tbody>
</table>